{
  "name": "vibecode-webgui",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node --require ./src/instrument.ts node_modules/.bin/next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:ws": "jest --testPathPattern=tests/integration/websocket",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:k8s": "jest --testPathPattern=tests/k8s",
    "test:k8s:quick": "jest --testPathPattern=tests/k8s/kind-cluster-validation\\.test\\.ts$ --maxWorkers=1",
    "test:k8s:helm": "jest --testPathPattern=tests/k8s/helm-chart-deployment\\.test\\.ts$ --maxWorkers=1",
    "test:k8s:provisioning": "jest --testPathPattern=tests/integration/user-provisioning-integration\\.test\\.ts$ --maxWorkers=1",
    "test:k8s:load": "k6 run tests/k8s/load-test.js",
    "test:monitoring": "jest --testPathPattern=tests/unit/.*monitoring.*\\.test\\.(ts|js)$ --testPathPattern=tests/integration/monitoring-api\\.test\\.(ts|js)$",
    "test:monitoring:unit": "jest --testPathPattern=tests/unit/.*monitoring.*\\.test\\.(ts|js)$",
    "test:monitoring:integration": "jest --testPathPattern=tests/integration/monitoring-api\\.test\\.(ts|js)$",
    "test:monitoring:e2e": "playwright test tests/e2e/monitoring-dashboard.test.ts",
    "test:monitoring:k8s": "jest --testPathPattern=tests/k8s/monitoring-deployment\\.test\\.(ts|js)$",
    "test:monitoring:security": "jest --testPathPattern=tests/security/monitoring-security\\.test\\.(ts|js)$",
    "test:complete": "jest --testPathPattern=tests/complete",
    "test:pre-commit": "./scripts/pre-commit-tests.sh",
    "test:monitoring:production": "jest --testPathPattern=tests/production/.*monitoring.*\\.test\\.(ts|js)$",
    "test:monitoring:health": "jest --testPathPattern=tests/production/monitoring-health\\.test\\.(ts|js)$",
    "test:monitoring:chaos": "jest --testPathPattern=tests/production/monitoring-production\\.test\\.(ts|js)$ --testNamePattern=\"Chaos Engineering\"",
    "test:monitoring:performance": "jest --testPathPattern=tests/production/monitoring-production\\.test\\.(ts|js)$ --testNamePattern=\"Performance Under Load\"",
    "test:datadog:real": "node scripts/test-datadog.js",
    "test:datadog:integration": "ENABLE_DATADOG_INTEGRATION_TESTS=true npm run test:monitoring:integration",
    "test:experiments": "jest --testPathPattern=tests/.*experiments.*\\.test\\.(ts|js)$",
    "test:experiments:unit": "jest --testPathPattern=tests/unit/.*experiments.*\\.test\\.(ts|js)$",
    "test:experiments:integration": "jest --testPathPattern=tests/integration/experiments-api\\.test\\.(ts|js)$",
    "test:ai-workflow": "jest --testPathPattern=tests/integration/ai-project-generation\\.test\\.ts$|tests/integration/workspace-creation\\.test\\.ts$",
    "test:ai-components": "jest --testPathPattern=tests/unit/ai-project-generator\\.test\\.tsx$",
    "test:ai-e2e": "playwright test tests/e2e/ai-project-workflow.test.ts",
    "test:collaboration": "jest --testPathPattern=tests/.*collaboration.*",
    "test:collaboration:unit": "jest tests/unit/collaboration.test.ts",
    "test:collaboration:integration": "jest tests/integration/collaboration-integration.test.ts",
    "test:docker": "jest --testPathPattern=tests/docker/.*\\.test\\.(ts|js)$",
    "test:docker:setup": "jest --testPathPattern=tests/docker/docker-setup\\.test\\.(ts|js)$",
    "test:docker:health": "jest --testPathPattern=tests/docker/container-health\\.test\\.(ts|js)$",
    "test:no-docker": "./scripts/test-without-docker.sh",
    "test:real-apis": "ENABLE_REAL_AI_TESTS=true npm run test:no-docker",
    "samples:populate": "npx tsx scripts/populate-vector-db-samples.ts",
    "verify:rag": "npx tsx scripts/verify-rag-functionality.ts",
    "db:setup-datadog": "ts-node scripts/setup-datadog-dbm.ts",
    "test:security": "./scripts/check-licenses.sh && ./scripts/vulnerability-scan.sh",
    "test:licenses": "./scripts/check-licenses.sh",
    "security:scan": "npm run test:security",
    "security:licenses": "npm run test:licenses",
    "security:deps": "npm audit",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:dev": "./scripts/kind-setup.sh setup",
    "k8s:cleanup": "./scripts/kind-setup.sh cleanup",
    "k8s:logs": "kubectl logs -f deployment/code-server -n vibecode-webgui",
    "server": "node server/index.js",
    "deploy": "node scripts/deploy.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=.next",
    "deploy:railway": "railway deploy",
    "deploy:docker": "docker build -f Dockerfile.production -t vibecode .",
    "deploy:docker:run": "docker run -p 3000:3000 vibecode",
    "deploy:compose": "docker-compose -f docker-compose.production.yml up -d",
    "health": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^0.0.1",
