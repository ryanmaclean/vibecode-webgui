{
  "name": "vibecode-webgui",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:ws": "jest --testPathPattern=tests/integration/websocket",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:k8s": "jest --testPathPattern=tests/k8s",
    "test:k8s:load": "k6 run tests/k8s/load-test.js",
    "test:security": "./scripts/check-licenses.sh && ./scripts/vulnerability-scan.sh",
    "test:licenses": "./scripts/check-licenses.sh",
    "security:scan": "npm run test:security",
    "security:licenses": "npm run test:licenses",
    "security:deps": "npm audit",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "k8s:dev": "./scripts/kind-setup.sh setup",
    "k8s:cleanup": "./scripts/kind-setup.sh cleanup",
    "k8s:logs": "kubectl logs -f deployment/code-server -n vibecode-webgui",
    "server": "node server/index.js"
  },
  "dependencies": {
    "next": "15.3.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5",
    "@xterm/xterm": "^5.5.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/addon-attach": "^0.11.0",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.427.0",
    "next-auth": "^4.24.7",
    "pg": "^8.12.0",
    "redis": "^4.7.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.5.2",
    "ws": "^8.18.0",
    "node-pty": "^1.0.0",
    "chokidar": "^3.6.0",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "overrides": {
    "sharp": false,
    "@img/sharp-libvips-darwin-arm64": false,
    "@img/sharp-libvips-linux-x64": false,
    "@img/sharp-libvips-win32-x64": false
  },
  "resolutions": {
    "sharp": false
  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@tailwindcss/postcss": "^4",
    "tailwindcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "@eslint/eslintrc": "^3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "jest-environment-jsdom": "^29.7.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.5.2",
    "@playwright/test": "^1.48.0",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "testcontainers": "^10.16.0",
    "@testcontainers/postgresql": "^10.16.0",
    "@testcontainers/redis": "^10.16.0",
    "socket.io-client": "^4.7.5",
    "msw": "^2.6.4",
    "prisma": "^5.18.0"
  }
}