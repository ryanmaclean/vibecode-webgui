    "lucide-react": "^0.469.0",
    "next": "^15.3.5",
    "next-auth": "5.0.0-beta.25",
    "next-themes": "^0.4.3",
    "openai": "^4.76.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.0",
    "react-resizable-panels": "^2.1.7",
