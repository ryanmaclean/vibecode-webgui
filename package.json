    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dd-trace": "5.58.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "lightningcss": "1.30.1",
    "lucide-react": "^0.427.0",
