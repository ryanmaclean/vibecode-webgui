{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.SemanticKernel": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "TimestampFormat": "yyyy-MM-dd HH:mm:ss ",
      "LogToStandardErrorThreshold": "Error"
    }
  },
  "SemanticKernel": {
    "DefaultModel": "gpt-4",
    "MaxTokens": 4000,
    "Temperature": 0.7,
    "TopP": 1.0,
    "FrequencyPenalty": 0.0,
    "PresencePenalty": 0.0,
    "StopSequences": [],
    "EnableFunctionCalling": true,
    "RetryPolicy": {
      "MaxRetries": 3,
      "DelayBetweenRetriesMs": 1000
    }
  },
  "OpenAI": {
    "ModelId": "gpt-4",
    "ChatModelId": "gpt-4",
    "EmbeddingModelId": "text-embedding-ada-002",
    "MaxTokens": 4000,
    "Temperature": 0.7,
    "TopP": 1.0,
    "FrequencyPenalty": 0.0,
    "PresencePenalty": 0.0
  },
  "AzureOpenAI": {
    "DeploymentName": "gpt-4",
    "ChatDeploymentName": "gpt-4",
    "EmbeddingDeploymentName": "text-embedding-ada-002",
    "ApiVersion": "2024-02-01"
  },
  "Plugins": {
    "EnabledPlugins": [
      "VibeCodePlugin",
      "TimePlugin",
      "CorePlugin"
    ],
    "PluginSettings": {
      "VibeCodePlugin": {
        "MaxComponentSize": 10000,
        "SupportedFrameworks": ["React", "Vue", "Angular"],
        "DefaultFramework": "React"
      },
      "TimePlugin": {
        "DefaultTimezone": "UTC",
        "DateFormat": "yyyy-MM-dd HH:mm:ss"
      }
    }
  },
  "Agent": {
    "Name": "VibeCode Assistant",
    "Description": "AI-powered development assistant for the VibeCode platform",
    "Instructions": "You are an expert AI development assistant for VibeCode. Help with code generation, reviews, architecture decisions, and best practices. Always prioritize security, performance, and maintainability.",
    "MaxConversationHistory": 50,
    "EnableMemory": true,
    "EnableWebSearch": false
  },
  "Security": {
    "EnableContentFiltering": true,
    "EnableInputValidation": true,
    "MaxInputLength": 10000,
    "AllowedFileTypes": [".js", ".ts", ".jsx", ".tsx", ".py", ".java", ".cs", ".go", ".rs", ".cpp", ".c", ".html", ".css", ".json", ".yaml", ".md"],
    "BlockedPatterns": ["<script>", "javascript:", "data:"]
  },
  "Performance": {
    "EnableCaching": true,
    "CacheDurationMinutes": 60,
    "MaxConcurrentRequests": 10,
    "RequestTimeoutSeconds": 30
  },
  "Telemetry": {
    "EnableMetrics": true,
    "EnableTracing": true,
    "EnableLogging": true,
    "ApplicationInsights": {
      "InstrumentationKey": "",
      "ConnectionString": ""
    },
    "Datadog": {
      "ApiKey": "",
      "Service": "vibecode-semantic-kernel",
      "Environment": "development",
      "Version": "1.0.0"
    }
  }
}